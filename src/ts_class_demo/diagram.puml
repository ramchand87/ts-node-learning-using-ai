@startuml TSClass
title TypeScript Class Diagram

class Person {
  +name: string
  +age: number
  +constructor(name: string, age: number)
  +greet(): void
}

note right of Person
  Exported using
  export class Person
end note

object "person1" as p1 {
  name = "Alice"
  age = 30
}

object "person2" as p2 {
  name = "Bob"
  age = 25
}

Person ..> p1 : Instantiates
Person ..> p2 : Instantiates

@enduml
